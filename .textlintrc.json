{
  "filters": {
    "comments": true
  },
  "rules": {
    "preset-japanese": true,
    "preset-ja-spacing": true,
    "ja-space-between-half-and-full-width": {
      "space": "always"
    },
    "@proofdict/proofdict": {
      "dictURL": "https://azu.github.io/proof-dictionary/"
    },
    "preset-ja-technical-writing": true,
    "spellcheck-tech-word": true
  },
  "plugins": ["review"]
}
